// constructs endings based on header and pages

construct = (a = 1) => {
    e.page = e.page + a < e.pages.length ? e.page + a : 0
    let html = e.header;
    html += e.pages[e.page] + "<br>";
    if (e.page > 0) {
      html += `<button onclick='endingConstructor(a = -1)'>Back</button>`
    }
    if (e.page < e.pages.length - 1) {
      html += `<button onclick='endingConstructor(a = 1)'>Next</button>`
    }
  
    if (e.image == true)
      setTimeout(()=>{
        candImg = $(".person_image")[0]
        if (candImg) {
          candImg.remove()
          $("#final_results_description")[0].style = `
            text-align:left;
            width: 70%;
            text-overflow: ellipsis;
            display: block;
            margin-left: auto;
            margin-right: auto;
          `
        }
      }, 10)
    else if (e.image)
    setTimeout(()=>{
        candImg = $(".person_image")[0];
        if (candImg)
          candImg.src = e.image;
      }, 10)
  
    return html;
  }
  
  endingConstructor = (a = 1) => {
    $("#final_results_description")[0].innerHTML = construct(a);
  }
  
// counts how many controversial answers a player makes & has an extra function to count different aspects of answers

controversyCount = (array, call = a => a.fields.controversial) => { 
  array = array.filter(a=>a);
  let answers = e.answers_json.map(f=>f.pk);
  let controversy = 0;
  for (i in array) {
  	let answer = e.answers_json[answers.indexOf(array[i])];
  	if (!call(answer)) {
      	continue;
  	}
  	controversy++;
  }
  return controversy;
}
// ENDINGS -- this is slightly more reliable than the ending code method

e.multiple_endings = true;


endingPicker = (out, totv, aa, quickstats) => {
  //out = "win", "loss", or "tie" for your candidate
  //totv = total votes in entire election
  //aa = all final overall results data
  //quickstat = relevant data on candidate performance (format: your candidate's electoral vote count, your candidate's popular vote share, your candidate's raw vote total)

  let controv = controversyCount(e.player_answers);
  let suicide = controversyCount(e.player_answers, a => a.fields.hell_answer);

  /*
  e.header = "<h2></h2>"
  e.pages = ["<p></p>",
         	"<p></p>"]
  */

  // JOHNSON CORRUPTION ENDINGS

  if ((suicide >= 1 || controv >= 1) && out == "loss") { 
	e.header = "<h2>Things Could Be Worse</h2>"
	e.pages = ["<p>As the results come in, you find yourself actually sighing with relief that by whether or small or large margin, you've come out of this short of an electoral majority. One might ask just why a president ousted from power could find relief in such a thing, but as it turns out, your deals with Johnson found themselves eyed by both state and federal investigators, who noticed the sudden swing in the polls towards you at the last minute from the Lone Star State. This would have been only a minor issue, if not for the recent controversy of the Senate Primary between Stevenson and Johnson. Already, you could hear the sounds of an inquiry, should you have come out of this election as the surprising victor.</p><p>But thankfully, your loss here today manages to shift their attention away from such facts as it seems to be a non-issue anyway, with the guaranteed election of a new president. With a sigh of relief, you take a seat in your chair, ironically happy that you'll simply be a one-term president who can perhaps continue the fight of progressive values, not behind the bars of a maximum security prison. Neither Eleanor nor Ilo have any idea why you seem to be in such high spirits, but they don't need to know about that.</p>"]
	return construct(0);
  }
  if ((suicide >= 1 || controv >= 1) && out == "tie") { 
	e.header = "<h2>Things Could Be Worse</h2>"
	e.pages = ["<p>As the results come in, you find yourself actually sighing with relief that by whether or small or large margin, you've come out of this short of an electoral majority. One might ask just why a president ousted from power could find relief in such a thing, but as it turns out, your deals with Johnson found themselves eyed by both state and federal investigators, who noticed the sudden swing in the polls towards you at the last minute from the Lone Star State. This would have been only a minor issue, if not for the recent controversy of the Democratic Senate Primary. Already, you could hear the sounds of an inquiry, should you have come out of this election as the surprising victor.</p><p>But thankfully, your loss here today manages to shift their attention away from such facts as it seems to be a non-issue anyway, with the guaranteed election of a new president. With a sigh of relief, you take a seat in your chair, ironically happy that you'll simply be a one-term president who can perhaps continue the fight of progressive values, not behind the bars of a maximum security prison. Neither Eleanor nor Ilo have any idea why you seem to be in such high spirits, but they don't need to know about that.</p>"]
    e.image = "https://npg.si.edu/media/B0T00144A.jpg";
	return construct(0);
  }
  if ((suicide >= 1 || controv >= 1) && out == "win") { 
	e.header = "<h2>CORRUPTION IN TEXAS; PRESIDENT WALLACE COLLUDES IN BALLOT STUFFING?!</h2>"
	e.pages = ["<p>This headline grips the front page of the <em>Texas State Gazette</em> and you nearly fall from your chair. You knew the risks, but this wasn’t how things were supposed to go down! Maybe it was the fact you won the state while having Eleanor on your ticket, but dammit. No one was supposed to figure it out! Now you’re hit with numerous accusations of electoral fraud, and slowly but surely, the walls are closing in. Johnson’s already seeking a plea bargain in exchange for testifying, and many in your own circle are looking for a way out, as Congress begins a set of Articles of Impeachment against you.</p><p> Your only hope now is to somehow get out of this without going to prison for the rest of your days. Perhaps you could resign in exchange for the promise of the Presidency to the current Speaker of the House, Joseph W. Martin, or if you hold on long enough, Eleanor herself could serve as the first female President. All of this is one massive longshot, but it may just be the only way you can survive this situation without a being in a prison suit. Get ready for a tumultuous few months Mr. President, because your career is effectively over. Only time will tell just how long it takes for everything to sink in.</p>"]
	return construct(0);
  }

  // NORMAL ENDINGS

