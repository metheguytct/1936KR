campaignTrail_temp.candidate_image_url = 'https://i.postimg.cc/8zYZRRKM/robert-taft-1948.jpg';
campaignTrail_temp.running_mate_image_url = 'https://i.postimg.cc/htsPG0r8/Untitled-design-18.png';
