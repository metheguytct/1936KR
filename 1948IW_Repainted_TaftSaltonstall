campaignTrail_temp.running_mate_last_name = 'Saltonstall';
campaignTrail_temp.running_mate_state_id = '352';
campaignTrail_temp.player_answers = [];
campaignTrail_temp.player_visits = [];
campaignTrail_temp.answer_feedback_flg = 1;
campaignTrail_temp.game_start_logging_id = '3668400';

// constructs endings based on header and pages

construct = (a = 1) => {
  e.page = e.page + a < e.pages.length ? e.page + a : 0
  let html = e.header;
  html += e.pages[e.page] + "<br>";
  if (e.page > 0) {
    html += `<button onclick='endingConstructor(a = -1)'>Back</button>`
  }
  if (e.page < e.pages.length - 1) {
    html += `<button onclick='endingConstructor(a = 1)'>Next</button>`
  }

  if (e.image == true)
    setTimeout(()=>{
      candImg = $(".person_image")[0]
      if (candImg) {
        candImg.remove()
        $("#final_results_description")[0].style = `
          text-align:left;
          width: 70%;
          text-overflow: ellipsis;
          display: block;
          margin-left: auto;
          margin-right: auto;
        `
      }
    }, 10)
  else if (e.image)
  setTimeout(()=>{
      candImg = $(".person_image")[0];
      if (candImg)
        candImg.src = e.image;
    }, 10)

  return html;
}

endingConstructor = (a = 1) => {
  $("#final_results_description")[0].innerHTML = construct(a);
}

// counts how many controversial answers a player makes & has an extra function to count different aspects of answers

controversyCount = (array, call = a => a.fields.controversial) => { 
  array = array.filter(a=>a);
  let answers = e.answers_json.map(f=>f.pk);
  let controversy = 0;
  for (i in array) {
  	let answer = e.answers_json[answers.indexOf(array[i])];
  	if (!call(answer)) {
      	continue;
  	}
  	controversy++;
  }
  return controversy;
}
// ENDINGS -- this is slightly more reliable than the ending code method

e.multiple_endings = true;


endingPicker = (out, totv, aa, quickstats) => {
  //out = "win", "loss", or "tie" for your candidate
  //totv = total votes in entire election
  //aa = all final overall results data
  //quickstat = relevant data on candidate performance (format: your candidate's electoral vote count, your candidate's popular vote share, your candidate's raw vote total)

  let controv = controversyCount(e.player_answers);
  let suicide = controversyCount(e.player_answers, a => a.fields.hell_answer);

  /*
  e.header = "<h2></h2>"
  e.pages = ["<p></p>",
         	"<p></p>"]
  */

  // MARTHA DEATH

  if ((suicide >= 1 || controv >= 1) && out == "loss") { 
	e.header = "<h2>I’m Coming Home Martha</h2>"
	e.pages = ["<p>It was all for nothing. All that blood, all that sweat, and even the very life of your wife was unable to bring you the greatest prize in the nation. Up to the day of the election the pain in your chest had been throbbing like mad, but now that same agony encapsulated your entire body as you stumbled to the tombstone where the light of life had been buried. Perhaps this was only fair, you had practically abandoned your wife for campaigning, and now it was coming back to haunt you every single moment. Every night in bed nothing but guilt and loneliness since the day she was taken from you, it was hell on earth, but now it was finally coming to an end. Collapsing next to her tombstone your body gives out a few more gasps as your heart begins to collapse, a shaky hand laid on the last loving tribute to the only woman you loved. ‘I love you.’ And as you close your eyes, you could swear she was laying right next to you.</p><p>The next day news outlets spread the word of your sudden passing away, many go on to mourn the loss of your life, even your hated enemy Thomas Dewey himself who manages to put personal politics aside to give a few solemn words at this tragic event. Things may not have turned out as you would have wanted, but at least your memory can still live on in the hearts of your supporters. Farewell Mr. Republican, for you shall be missed.</p>"]
    e.image = "https://upload.wikimedia.org/wikipedia/commons/thumb/1/1a/Sen._Robert_Taft_of_Ohio_LCCN2016878050.jpg/745px-Sen._Robert_Taft_of_Ohio_LCCN2016878050.jpg?20181025123759";
	return construct(0);
  }
  if ((suicide >= 1 || controv >= 1) && out == "win") { 
	e.header = "<h2>We did it Martha, we did it</h2>"
	e.pages = ["<p>The pain in your chest remains tight like a vice on your heart as you handle a box of roses in your hands. Despite the fact by all accounts you should be celebrating, giving speeches to your supporters, or even just planning out the transition of power, right now all you want to do is tell the news to the most important person in your life. The air is chilly but that doesn’t stop you from walking to the grave of Martha Taft, the very woman who had even brought you this far. Even with this success sometimes you simply spend hours wishing she was by you right this second. The memory of leaving her bed to campaign still a haunting mistake that will never leave you, no matter how many times even your dear first born son Robert Taft Jr assures you that there was nothing that could have been done.</p><p>But now isn’t the time to be buried by grief, and as you bring the flowers out of their box, a smile graces your lips at seeing despite being away no dust or dirt litters her tombstone. Bending a knee onto the ground, you happily tell your wife of the victory, how against so many obstacles you've finally become the president elect and can bring this country back on track. For nearly two hours you talk to her, relaying everything that had occurred throughout the campaign season. But soon your visit has to come to an end, and setting the roses on her grave, Robert Taft clutches a bolt of pain in his chest, and shrugs it off. ‘I promise I’ll be with you soon Martha, just wait for me.’ A ghostly arm hugging you as you leave.</p>"]
	return construct(0);
  }
  if ((suicide >= 1 || controv >= 1) && out == "tie") { 
	e.header = "<h2>I’m Coming Home Martha</h2>"
	e.pages = ["<p>It was all for nothing. All that blood, all that sweat, and even the very life of your wife was unable to bring you the greatest prize in the nation. Up to the day of the election the pain in your chest had been throbbing like mad, but now that same agony encapsulated your entire body as you stumbled to the tombstone where the light of life had been buried. Perhaps this was only fair, you had practically abandoned your wife for campaigning, and now it was coming back to haunt you every single moment. Every night in bed nothing but guilt and loneliness since the day she was taken from you, it was hell on earth, but now it was finally coming to an end. Collapsing next to her tombstone your body gives out a few more gasps as your heart begins to collapse, a shaky hand laid on the last loving tribute to the only woman you loved. ‘I love you.’ And as you close your eyes, you could swear she was laying right next to you.</p><p>The next day news outlets spread the word of your sudden passing away, many go on to mourn the loss of your life, even your hated enemy Thomas Dewey himself who manages to put personal politics aside to give a few solemn words at this tragic event. Things may not have turned out as you would have wanted, but at least your memory can still live on in the hearts of your supporters. Farewell Mr. Republican, for you shall be missed.</p>"]
    e.image = "https://upload.wikimedia.org/wikipedia/commons/thumb/1/1a/Sen._Robert_Taft_of_Ohio_LCCN2016878050.jpg/745px-Sen._Robert_Taft_of_Ohio_LCCN2016878050.jpg?20181025123759";
	return construct(0);
  }

  // NORMAL ENDINGS

  if (suicide < 1 && controv <= 1 && quickstats[0] < 81) { 
	e.header = "<h2>Like Father, Like Son</h2>"
	e.pages = ["You wanted to carry on the spirit of your father, former president William Howard Taft, but not in this way! Instead of emulating 1908 you’ve recaptured 1912 where somehow the Republicans have been beaten by a Third Party and come out of this in absolute shambles! But to make things even worse it seems unlikely the party will manage to bounce back so quickly as they did before, their losses in not only the House but the Senate are astronomical in number. Your once 51 Seat Majority in the highest chamber of Congress is now in the mid 30s thanks to terrible losses both in the East Coast and even in states like South Dakota and Nebraska. Handing the Democrats a nice majority in both chambers to pass whatever they want.</p><p>If there was any hope for the conservative wing before, now it’s officially dead in the water as once again that ideology is scorned by the public and your party for its massive failures. Hell even the moderate wing may be dead considering even with Saltonstall on the ticket the picture perfect moderate himself the both of you were absolutely crushed by two devout New Deal liberals. At this point it'll be a miracle if 1952 isn't Harold Stassen vs Henry Wallace with just how much room the liberals have to take over. Your worst nightmare is slowly coming true."]
	return construct(0);
  }
  if (suicide < 1 && controv <= 1 && quickstats[0] <= 151 && out == "loss") { 
	e.header = "<h2>A Republican's Titanic</h2>"
	e.pages = ["<p>How could it have come to this? Were you not conservative enough, did you try to compromise too much for the sake of electability> Whatever you did it has single handedly destroyed your political career. Initial projections showed you winning most states in the country and coming out of this with a massive mandate to pave the way for your agenda. Instead you’ve managed to fumble the bag so badly that even when on a unity ticket you've managed to destroy any chances of reclaiming the White House for the Republicans.</p><p>Already you can feel your grip on the Republican Party ripped away by the yankings of Harold Stassen and Earl Warren, both immediately in a battle to try and become the new leader of the Republicans as you’re quickly shuffled back off to Ohio. Even moderates such as Saltonstall and Arthur Vandenburg find themselves threatened by a growingly rabid liberal faction with Charles Potter gaining momentum to primary Vandenburg in 52. Whatever happens from here on out to the Republican Party, you can expect it not to be anything you're in agreement with.</p>"]
	return construct(0);
  }
  if (suicide < 1 && controv <= 1 && quickstats[0] <= 185 && out == "loss") { 
	e.header = "<h2>The Facade of Unity</h2>"
	e.pages = ["<p>As you see Wallace hang on to a narrow victory you realize just what kind of mistake you’ve made. You knew that sacrificing your ideology was a bad idea, but everyone, even John kept telling you that the only way to win was to unify the party. That there's no way a ticket of yourself and Dirksen as you'd been pushing for could ever win in this race. Oh what a bunch of hogwash!</p><p>Thankfully you did manage to perform somewhat decently so there might still be a change to hold a great sway over the platform of the future, but you can assuredly expect not only a challenge nationally from the left, but also a difficult battle in retaining your Senate seat in 1950. Unfortunately your allies prove limited as Leverett has stabbed you in the back claiming it was all your fault that the ticket had performed this poorly. Oh how you'll surely get him back for this even if it's the last thing you do!</p>"]
	return construct(0);
  }
  if (suicide < 1 && controv <= 1 && quickstats[0] <= 265 && out == "tie") { 
	e.header = "<h2>No Loyalty in Politics</h2>"
	e.pages = ["<p> 'They're double crossing us!' John yells as hears the word from his insiders on the Republican Party. Already words going around of forgoing any negotiations with the Democrats or S.R.P in the House so that they can easily nominate Saltonstall in the Senate for the presidency. Thanks to Wallace's own Running Mate the greenhorn Estes Kefauver even most Democrats are on board with promoting a sensible moderate over the untested New Deal Representative. Even many in your own camp are open to the idea after you've managed to alienate a good number of them with your own controversial decisions during the campaign. Your inability to compromise or make amends with the party coming back to bite you right in keister.</p><p>Things only get worse when you confront Leverett a few days after the election at his home in Massachusetts where he tells you in no uncertain terms, 'I've already discussed the idea with Senate Majority Leader White, and he's told me that they've already secured the votes to appoint me as Vice President.' A few minutes later you're stumbling away in shock, unable to believe perhaps your only opportunity as president has just been ripped away by the very party you loyally fought for.</p>"]
    e.image = "https://content.time.com/time/magazine/archive/covers/1944/1101440410_400.jpg";
	return construct(0);
  }
  if (suicide < 1 && controv <= 1 && quickstats[0] <= 399 && out == "win") { 
	e.header = "<h2>Taft Defeats Wallace!</h2>"
	e.pages = ["<p>It may have been a closer victory than anyone had expected, but that doesn’t take away anything from your historic victory. The Democrats have managed to close the gap in Congress turning your majority into a plurality, but this still leaves you room to pass your ambitious legislation. There will still be the need for compromises with the many factions both in and outside of your party, but by picking Saltonstall you've managed to regain faith in the party establishment and moderates giving you an opening to pass your agenda. This should be more than enough to push your isolationist rhetoric and hopefully get this country out of the quagmire that is Western Europe. Still the both of you aren't exactly on the tightest of terms so it may require some negotiation and growing pains before the two of you can function as a legislative unit.</p><p>But together with your friend Bricker, alongside your wife and children, the eight of you spend the night away celebrating your victory as the Taft dynasty continues to flourish. Who knows, maybe a successful two-term presidency could even lead to one of your own children someday taking the reigns as well. But that’s a long time away, for now it’s time to relax and then get to work on reshaping the country in your image!</p>"];
	return construct(0);
  }
  if (suicide < 1 && controv <= 1 && quickstats[0] <= 530 && out == "win") { 
	e.header = "<h2>To A New Dawn Robert</h2>"
	e.pages = ["<p>You hear those genuine words from the mouth of your once vitriolic adversary Thomas Dewey who finds himself forced to meet with you after such a landslide victory. Not only did you trounce Wallace and Thurmond, but you’ve managed to do so in embarrassing fashion to the point even New York was in contention! And much of that comes down to your own efforts to make amends with men like Dewey. The two of you for the past four years tearing each other apart, only to come together at the last minute for this overwhelming victory. They say irony's a bitch, and right now there's no better proof to that.</p><p>It takes a monumental amount of effort not to laugh at the fact you're having lunch with the man who once threatened to punch your lights out, yet as you sip on a bottle of expensive wine you manage to keep a calm demeanor. Of course that won’t stop you from relaying this entire scene at the dinner table the next night, after all if you can’t have a good laugh in your own home then when can you?</p>"];
    e.image = "https://i.ebayimg.com/images/g/nUsAAOSwthtfocuK/s-l1200.webp";
	return construct(0);
  }
  if (suicide < 1 && controv <= 1 && quickstats[0] >= 531) { 
	e.header = "<h2>FUCK THAT GUY!</h2>"
	e.pages = ["<p>You hear your soon to be Secretary of State Herbert Hoover gleefully cry as he sees the results. In a shocking event, you've won every single state and successfully gained massive conservative supermajorities in congress! With this you can easily bring America back to the prosperity of the roaring 20s before all this 'New Deal' nonsense and what other insanity both Roosevelt and Wallace have implemented during their time in office. Clearly the country knew it was time to reverse course from government intervention and back to the beauty of private enterprise! No doubt the next four years are going to see a massive change in not only the United States, but the world as well once you're finished enacting your grand vision. To the victor goes the spoils, and you certainly earned quite a lot.</p>"];
    e.image = "https://www.trumanlibrary.gov/sites/default/files/styles/tiff_conversion/public/photographs/64/64-722.tif.jpg?itok=oHpMO3DN";
